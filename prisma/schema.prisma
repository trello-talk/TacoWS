generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int      @id @default(autoincrement())

  @@map("users")
}

model Webhook {
  id              Int      @id @default(autoincrement())
  guildID         String   @db.VarChar(255)
  createdAt       DateTime @db.Timestamptz(6) @default(now())
  updatedAt       DateTime @db.Timestamptz(6) @updatedAt

  @@map("webhooks")
}
